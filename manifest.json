{
    "manifest_version": 3,
    "name": "Bundle Install Helper",
    "version": "1.0.0",
    "description": "Fills out the 'Bundle Install Preview' table for you.",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
    },
    "permissions": ["scripting", "tabs", "activeTab","storage"]
  }